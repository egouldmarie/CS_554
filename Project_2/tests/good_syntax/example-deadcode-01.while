{-
  example-deadcode-01.while
  program with several pieces of deadcode
-}
n := input;
steps := 0;
-- loop containing a meaningless (dead code) assignment
while n > 1 do
  skip;
  n := n - 1;
  skip;
  steps := steps + 1;
  k := 3  -- dead code
od;
-- if-then-else with a meaningless (dead code) assignment
if steps > 40 then
  k := 5; -- dead code
  steps := 40
else
  steps := steps - 1
fi;
