digraph CFG {
    rankdir=TB;
    node [shape=box, style=rounded];

    "node_entry" [label="ENTRY", fillcolor=green, style="rounded,filled"];
    "node_exit" [label="EXIT", fillcolor=red, style="rounded,filled"];
    "node_0" [label="Label 0\ny := ...", fillcolor=white, style="rounded,filled"];
    "node_1" [label="Label 1\nz := ...", fillcolor=white, style="rounded,filled"];
    "node_2" [label="Label 2\n[condition]", fillcolor=lightblue, style="rounded,filled"];
    "node_3" [label="Label 3\nz := ...", fillcolor=white, style="rounded,filled"];
    "node_4" [label="Label 4\ny := ...", fillcolor=white, style="rounded,filled"];
    "node_5" [label="Label 5\ny := ...", fillcolor=white, style="rounded,filled"];
    "node_6" [label="Label 6\noutput := ...", fillcolor=white, style="rounded,filled"];

    "node_entry" -> "node_0";
    "node_0" -> "node_1";
    "node_1" -> "node_2";
    "node_2" -> "node_3";
    "node_2" -> "node_5";
    "node_3" -> "node_4";
    "node_4" -> "node_2";
    "node_5" -> "node_6";
    "node_6" -> "node_exit";
}