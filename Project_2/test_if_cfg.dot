digraph CFG {
    rankdir=TB;
    node [shape=box, style=rounded];

    "node_entry" [label="ENTRY", fillcolor=green, style="rounded,filled"];
    "node_exit" [label="EXIT", fillcolor=red, style="rounded,filled"];
    "node_0" [label="Label 0\n[condition]", fillcolor=lightblue, style="rounded,filled"];
    "node_1" [label="Label 1\ny := ...", fillcolor=white, style="rounded,filled"];
    "node_2" [label="Label 2\ny := ...", fillcolor=white, style="rounded,filled"];
    "node_merge" [label="merge", fillcolor=yellow, style="rounded,filled"];

    "node_entry" -> "node_0";
    "node_0" -> "node_1";
    "node_0" -> "node_2";
    "node_1" -> "node_merge";
    "node_2" -> "node_merge";
    "node_merge" -> "node_exit";
}