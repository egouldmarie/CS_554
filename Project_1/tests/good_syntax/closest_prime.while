mod := input;
while not[mod=1 or mod=0] do
    mod:=mod-2
od;
if mod = 1 then
    input:= input - 2
else
    input:= input - 1
fi;
i := input;
stop1 := 0;
while i >= 2 and stop1 = 0 do
    sqrt := 1;
    while sqrt*sqrt <=i do
       sqrt:=sqrt+1
    od;
    j := 3;
    stop := 0;
    while j <= sqrt and stop = 0 do
        mod := i;
        while  mod > 0 do
            mod:=mod-j
        od;
        if mod = 0 then
           stop := 1
        else
            j := j+ 2
        fi
    od;
     if j > sqrt then
        stop1 := 1
     else
        i := i - 2
     fi
od;
closestprime := i;
output := closestprime

